<?xml version="1.0" encoding="UTF-8" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <view id="main">
    <page>/main.jsf</page>
  </view>
  <view id="index">
    <page>/index.jspx</page>
  </view>
  <view id="fileList">
    <page>/fileList.jsf</page>
  </view>
  <view id="fileLoad">
    <page>/fileLoad.jsf</page>
  </view>
  <view id="fileUnLoad">
    <page>/fileUnLoad.jsf</page>
  </view>
  <control-flow-rule id="__2">
    <from-activity-id>*</from-activity-id>
    <control-flow-case id="__3">
      <from-outcome>main</from-outcome>
      <to-activity-id>main</to-activity-id>
    </control-flow-case>
    <control-flow-case id="__6">
      <from-outcome>index</from-outcome>
      <to-activity-id>index</to-activity-id>
    </control-flow-case>
    <control-flow-case id="__5">
      <from-outcome>filelist</from-outcome>
      <to-activity-id>fileList</to-activity-id>
    </control-flow-case>
    <control-flow-case id="__7">
      <from-outcome>fileload</from-outcome>
      <to-activity-id>fileLoad</to-activity-id>
    </control-flow-case>
    <control-flow-case id="__8">
      <from-outcome>fileunload</from-outcome>
      <to-activity-id>fileUnLoad</to-activity-id>
    </control-flow-case>
  </control-flow-rule>
  <managed-bean id="__1">
    <managed-bean-name>login</managed-bean-name>
    <managed-bean-class>view.bean.LoginBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean id="__4">
    <managed-bean-name>menuBean</managed-bean-name>
    <managed-bean-class>view.bean.MenuBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
</adfc-config>
